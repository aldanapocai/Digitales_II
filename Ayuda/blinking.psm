; Implementar en assembly un desplazador a izquierda en funci√≥n del
;estado de los switches
;=============================================================
; Constantes
;=============================================================
CONSTANT leds_out		, 01    ; LED write port
CONSTANT sw_in			, 00    ; Switchs read port
CONSTANT clear_all		, 00	; Limpieza

;=============================================================
; Codigo assembly principal
;=============================================================
			LOAD	s3, 00
main:		OUTPUT	s3,	leds_out
			CALL	delay_1s
			LOAD	s3, FF
			OUTPUT	s3,	leds_out
			JUMP	delay_1s
			JUMP	main

delay_1s: 	LOAD s2, 7D
 			LOAD s1, E2
 			LOAD s0, 90
delay_loop: SUB s0, 1'd
 			SUBCY s1, 0'd
 			SUBCY s2, 0'd
 			JUMP NZ, delay_loop
 			return 

